

  <div class="eat-it">

    {{#if model}}
      {{#each model as |animal|}}
      {{#link-to 'animal' animal.id }}{{animal-tile animal=animal}}{{/link-to}}
      {{/each}}
    {{else}}
      <h3>Sorry, but the animals in this category have already been smuggled.</h3>
      <img class="z-depth-5 animal-pic" src="http://i245.photobucket.com/albums/gg62/witchyhoy3/AAA/animals-001.jpg" alt="happy puppy" />
    {{/if}}
  </div>
